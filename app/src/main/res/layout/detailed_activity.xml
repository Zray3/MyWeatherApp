<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/imagenDetails"
        android:layout_width="172dp"
        android:layout_height="151dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.027"
        app:srcCompat="@android:drawable/checkbox_off_background" />

    <TextView
        android:id="@+id/diaDetails"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textAllCaps="true"
        android:textColor="#070707"
        android:textSize="48sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imagenDetails"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/fechaDetails"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="TextView"
        android:textColor="#070707"
        android:textSize="20sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imagenDetails"
        app:layout_constraintTop_toBottomOf="@+id/diaDetails" />

    <TextView
        android:id="@+id/horaDetails"
        android:layout_width="142dp"
        android:layout_height="49dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.743"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imagenDetails"
        app:layout_constraintVertical_bias="0.099" />

    <TextView
        android:id="@+id/estadoCieloDetails"
        android:layout_width="142dp"
        android:layout_height="49dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/horaDetails"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/horaDetails"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/temperaturaDetails"
        android:layout_width="142dp"
        android:layout_height="49dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/temperaturaMinDetails"
        app:layout_constraintStart_toStartOf="@+id/temperaturaMinDetails"
        app:layout_constraintTop_toBottomOf="@+id/temperaturaMinDetails" />

    <TextView
        android:id="@+id/temperaturaMaxDetails"
        android:layout_width="142dp"
        android:layout_height="53dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/estadoCieloDetails"
        app:layout_constraintStart_toStartOf="@+id/estadoCieloDetails"
        app:layout_constraintTop_toBottomOf="@+id/estadoCieloDetails" />

    <TextView
        android:id="@+id/temperaturaMinDetails"
        android:layout_width="142dp"
        android:layout_height="49dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/temperaturaMaxDetails"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/temperaturaMaxDetails"
        app:layout_constraintTop_toBottomOf="@+id/temperaturaMaxDetails" />

    <TextView
        android:id="@+id/sensacionTermicaDetails"
        android:layout_width="142dp"
        android:layout_height="49dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/temperaturaDetails"
        app:layout_constraintStart_toStartOf="@+id/temperaturaDetails"
        app:layout_constraintTop_toBottomOf="@+id/temperaturaDetails" />

    <TextView
        android:id="@+id/humedadDetails"
        android:layout_width="142dp"
        android:layout_height="49dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/sensacionTermicaDetails"
        app:layout_constraintStart_toStartOf="@+id/sensacionTermicaDetails"
        app:layout_constraintTop_toBottomOf="@+id/sensacionTermicaDetails" />

    <TextView
        android:id="@+id/presionDetails"
        android:layout_width="142dp"
        android:layout_height="49dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/humedadDetails"
        app:layout_constraintHorizontal_bias="0.606"
        app:layout_constraintStart_toStartOf="@+id/humedadDetails"
        app:layout_constraintTop_toBottomOf="@+id/humedadDetails" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="142dp"
        android:layout_height="49dp"
        android:text="Hora:"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/horaDetails"
        app:layout_constraintEnd_toStartOf="@+id/horaDetails"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/horaDetails" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="142dp"
        android:layout_height="49dp"
        android:text="Cielo:"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/estadoCieloDetails"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/estadoCieloDetails" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="142dp"
        android:layout_height="49dp"
        android:layout_marginTop="2dp"
        android:text="T.Máxima:"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/temperaturaMaxDetails"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/temperaturaMaxDetails" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="142dp"
        android:layout_height="49dp"
        android:text="T.Mínima"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/temperaturaMinDetails"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/temperaturaMinDetails" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="142dp"
        android:layout_height="49dp"
        android:text="Temperatura:"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/temperaturaDetails"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/temperaturaDetails" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="142dp"
        android:layout_height="49dp"
        android:text="Sensación.T:"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/sensacionTermicaDetails"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/sensacionTermicaDetails" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="142dp"
        android:layout_height="49dp"
        android:text="Humedad:"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/humedadDetails"
        app:layout_constraintEnd_toEndOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="@+id/humedadDetails" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="142dp"
        android:layout_height="49dp"
        android:text="Presión:"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/presionDetails"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/presionDetails" />

</androidx.constraintlayout.widget.ConstraintLayout>